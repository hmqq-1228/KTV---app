	border-radius: 50%;
}
.item.syy-chat span{
	float:right;
	height:60px;
	padding:10px;
	margin-top:10px;
	border-radius: 6px;
	line-height: 40px;
	background-color:#E0E0E0;
	position:relative;
}
.item.syy-chat span .sj{
	border:6px solid transparent;
	border-left-color:#E0E0E0;
	position:absolute;
	top:50%;
	right:-12px;
	margin-top:-6px;
}
